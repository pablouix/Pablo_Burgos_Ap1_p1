<Window x:Class="Pablo_Burgos_Ap1_p1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Pablo_Burgos_Ap1_p1"
        mc:Ignorable="d"
        Title="Sistema de gestion productos" Height="570" Width="1100">
    <Grid>
        <StackPanel>
            <Menu>
            
                <MenuItem Header="Registros">
                    <MenuItem Header ="Productos" x:Name="rRegistro" Click="RegistroRegistrosMenuItem_Click" />
                </MenuItem>

                <MenuItem Header="Consultas">
                    <MenuItem Header="Productos" x:Name = "cConsulta" Click="ConsultaConsultasMenuItem_Click" />
                </MenuItem>

            </Menu>
        </StackPanel>
    </Grid>
</Window>
